{
  "name": "byteball",
  "description": "Smart payments made simple",
  "author": "Byteball",
  "version": "1.10.0",
  "keywords": [
    "wallet",
    "byteball",
    "decentralized",
    "multisignature",
    "bitcoin"
  ],
  "main": "public/index.html",
  "window": {
    "title": "Byteball wallet",
    "icon": "./public/img/icons/icon-white-256.png",
    "show": true,
    "visible": true,
    "resizable": false,
    "frame": true,
    "width": 400,
    "height": 600,
    "position": "center",
    "fullscreen": false
  },
  "webkit": {
    "page-cache": false,
    "java": false,
    "plugin": false
  },
  "chromium-args": "--proxy-server=http://127.1.2.7 --password-store=basic",
  "homepage": "https://github.com/byteball/byteball",
  "license": "MIT",
  "repository": {
    "url": "git://github.com/byteball/byteball.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/byteball/byteball/issues"
  },
  "browser": {
    "request": "browser-request",
    "secp256k1": "secp256k1/js"
  },
  "dependencies": {},
  "optionalDependencies": {},
  "scripts": {
    "test": "./node_modules/.bin/grunt test-coveralls",
    "mac": "",
    "android": "",
    "grunt": "node_modules/.bin/grunt",
    "build": "yarn grunt; yarn nw;",
    "yarn": "yarn; yarn clean; yarn upgrade; yarn install;",
    "bower": "node_modules/.bin/bower cache clean; node_modules/.bin/bower update; node_modules/.bin/bower install",
    "upd": "yarn yarn; yarn bower",
    "in": "yarn upd; yarn sqfix; yarn grunt; yarn nw;",
    "nw": "node_modules/.bin/nw .",
    "sqfix": "mv node_modules/sqlite3/lib/binding/node-v57-darwin-x64 node_modules/sqlite3/lib/binding/node-webkit-v0.24.0-darwin-x64"
  },
  "devDependencies": {
    "adm-zip": ">=0.0",
    "angular": "1.4.6",
    "angular-carousel": ">=0.0",
    "angular-elastic": ">=0.0",
    "angular-gettext": ">=0.0",
    "angular-mocks": ">=0.0",
    "angular-moment": ">=0.0",
    "angular-qrcode": "monospaced/angular-qrcode",
    "angular-touch": "1.4.6",
    "angular-ui-router": ">=0.0",
    "angular-ui-switch": ">=0.0",
    "animate.css": ">=0.0",
    "async": ">=0.0",
    "bhttp": ">=0.0",
    "bip38": ">=0.0",
    "bitcore-lib": ">=0.0",
    "bitcore-mnemonic": ">=0.0",
    "bower": ">=0.0",
    "byteballcore": "byteball/byteballcore",
    "fastclick": ">=0.0",
    "foundation": ">=0.0",
    "foundation-icon-fonts": ">=0.0",
    "grunt": ">=0.0",
    "grunt-angular-gettext": ">=0.0",
    "grunt-browserify": ">=0.0",
    "grunt-contrib-compress": ">=0.0",
    "grunt-contrib-concat": ">=0.0",
    "grunt-contrib-copy": ">=0.0",
    "grunt-contrib-uglify": ">=0.0",
    "grunt-contrib-watch": ">=0.0",
    "grunt-exec": ">=0.0",
    "grunt-karma": ">=0.0",
    "grunt-karma-coveralls": ">=0.0",
    "grunt-nw-builder": ">=0.0",
    "innosetup-compiler": ">=0.0",
    "jasmine-core": ">=0.0",
    "jszip": ">=0.0",
    "karma": ">=0.0",
    "karma-cli": ">=0.0",
    "karma-coverage": ">=0.0",
    "karma-jasmine": ">=0.0",
    "karma-phantomjs-launcher": ">=0.0",
    "lodash": ">=0.0",
    "moment": ">=0.0",
    "ng-lodash": "0.2.3",
    "nw": ">=0.0",
    "preconditions": ">=0.0",
    "qrcode-decoder-js": "colkito/qrcode-decoder-js",
    "shelljs": ">=0.0",
    "sjcl": ">=0.0",
    "ui-router-extras": ">=0.0",
    "unzip": "byteball/unzip",
    "zip": "xJeneKx/zip"
  }
}
